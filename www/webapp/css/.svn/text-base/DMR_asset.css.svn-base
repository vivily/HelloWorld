body {
background-color: #888888;
font-family: arial;
height: 2000px;
padding: 0px;
margin: 0px;

}
#content_panel {
	background-color: #f1f6f7;
	width: 740px;
	height: 100%;
	padding-bottom: 100px;
	padding-left: 30px;
	padding-top: 10px;
	padding-right: 10px;
	margin: 0px;
	margin-left: 30px;
	-webkit-box-shadow: 10px 0px 7px #555555;
	-moz-box-shadow: 7px 0px 7px #000000;
	font-size: 11px;
}
#asset_edit_panel
   {

   padding: 20px;
   width: 650px;
   background: #c3d7e3;
   border: #404040 0px solid;
   border-top-left-radius: 4px;
   border-top-right-radius: 4px;
   border-bottom-left-radius: 10px;
   border-bottom-right-radius: 10px;
   -webkit-box-shadow: 7px 7px 7px #333333;
   -moz-box-shadow: 7px 7px 7px #333333;
   
   display: "none";
   padding-bottom: 65px;
}

#record
{
	border-left: 1px solid #AAAAAA;
	border-top: 1px solid #AAAAAA;
	border-radius: 4px;
	padding: 10px;
	-webkit-box-shadow: 4px 4px 4px #555555;
	-moz-box-shadow: 4px 4px 4px #555555;
}

.bold_title
{
	font-weight: bold;
	font-size: 18pt;
	margin: 5px;
}

.bold_medium
{
	font-weight: bold;
	font-size: 16px;
}

.bold_cell
{
	font-weight: bold;
	font-size: 14px;
	color: #485054;
}



.asset_prop_table
{
	border: 0px solid ;
	table-layout:fixed;
	font-size: 13px;
}

td, th {
	padding: 1px;
	height: 17px;
	font-size: 12px;
	color: #303030;
	overflow: hidden;
	text-overflow: clip;
}
.rFieldId {
	border:0;
	height: 18px;
	font-size: 12px;
	color: #303030;
	overflow: hidden;
	text-overflow: clip;
}
th {
	background: #92a1aa;
}

td[ContentEditable="true"] {
	background: #FFFFFF; 
	border: 2px solid #90a0aa;
}

.long_field {
	width: 250px;
}
.long_field_id {
	width: 250px;
}

#publish_btn, #export_btn, #import_btn, #save_btn
{
	border: 1px solid #999999;
    width: 85px;
    height: 20px;
    background: #CCCCCC;
	-webkit-box-shadow: 2px 2px 2px #999999;
    -moz-box-shadow: 2px 2px 2px #999999;
    border-radius: 3px;
    text-align: center;
    font-weight: bold;
    padding: 4px;
	float:left;
    margin: 15px;
    margin-top: 25px;
}

#asset_physical_table {
    display: "none";
    }

select, input {
	height: 26px;
}

input[type="number"]
{
	width: 50px;
}

#record_list{
	border: 1px solid #dddddd;
}

#AssetSummary, #search_result_panel {
	background: white;
	border: 2px solid #92a1aa;
	position: absolute;
	/*display: none;*/
	left: 100px;
	top: 175px;
	width: 625px;
	//height: 340px;
	opacity: .95;
	border-radius: 10px;
	display: none;
	overflow: auto;
	text-overflow: clip;
	word-wrap: break-word;
	padding: 10px;
	-webkit-box-shadow: 4px 4px 4px #888888;
	-moz-box-shadow: 4px 4px 4px #888888;
}

#advanced_search_panel {
	background: white;
	border: 2px solid #92a1aa;
	position: absolute;
	left: 320px;
	top: 50px;
	width: 400px;
	border-radius: 4px;
	display: block;
	overflow: auto;
	text-overflow: clip;
	word-wrap: break-word;
	padding: 10px;
	-webkit-box-shadow: 4px 4px 4px #888888;
	-moz-box-shadow: 4px 4px 4px #888888;
	display: none;
}
#AssetSerialized, #result_content {
	background: white;
	border: 1px solid #aaaaaa;
	margin: 20px;
	margin-top: 10px;
	width: 570px;
	height: 240px;
	overflow: auto;
	text-overflow: clip;
	word-wrap: break-word;
	padding: 10px;
}
#yes_btn, #cancel_btn, #ok_btn {
	width: 100px;
    height: 20px;
	-webkit-box-shadow: 4px 4px 4px #92a1aa;
    -moz-box-shadow: 4px 4px 4px #92a1aa;
    border: 1px solid #92a1aa;
    border-radius: 2px;
    text-align: center;
    font-weight: bold;
    padding: 4px;
    margin: 6px;
    margin-left: 60px;
    margin-bottom: 16px;
    float:left;
 }
#moving_row {
	border: 2px solid black;
	position: absolute;
	left: 100px;
	top: 200px;
	width: 500px;
	height: 40px;
	display: none;
}

#saved_info
{
	display: block;
	float:left;
	font-size: 9px;
}

#published_info
{
	display: block;
	float:left;
	font-size: 13px;
}

#bottom_btns
{
	height: 60px;
}

#search_ui{
	float: right;
	clear: both;
}

#search_text {
	height: 18px;
}

.Asset_prop_tab {
	border-top: 1px solid #555555;
	border-right: 1px solid #555555;
	border-left: 1px solid #555555;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	float:left;
	background: #c9d0d5;
	width: 120px;
	height: 20px;
	padding: 5px;
	-webkit-box-shadow: 3px 0px 3px #555555;
	-moz-box-shadow: 5px 0px 3px #555555;
	text-align: center;
	font-weight: bold;
	font-size: 13px;
}
.table_tabs {
	height: 30px;
	clear: both;
}
.prop_tables {
	clear: both;
	border: 2px #777777 solid;
	width: 550px;
}

#record_list  {
	overflow-y: auto;
	overflow-x: hidden;
	width: 680px;
	height: 150px; 
	padding-left: 3px;
}

tbody{
border-spacing: 0px;
}

.Asset_list_tab {
	font-size:13px;
	border-top: 1px solid #555555;
	border-right: 1px solid #555555;
	border-left: 1px solid #555555;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	float:left;
	background: #e5eecc;
	width: 140px;
	height: 20px;
	padding: 5px;
	-webkit-box-shadow: 3px 0px 3px #555555;
	-moz-box-shadow: 5px 0px 3px #555555;
	text-align: center;
	font-weight: bold;
}

#list_area {
	border-left: 1px solid #aaaaaa;
	border-bottom: 1px solid #aaaaaa;
	width: 683px;
}

#list_all
{
	background: #abb299;
}

#list_tabs {
	height: 20px;
	clear: both;
}